"Loose Methods"!

!CefJavaScriptException class methodsFor!

fromDictionary: aDictionary
	^self new
		message: (aDictionary at: #message);
		lineNumber: (aDictionary at: #lineNumber);
		startColumn: (aDictionary at: #startColumn);
		startPosition: (aDictionary at: #startPosition);
		endColumn: (aDictionary at: #endColumn);
		endPosition: (aDictionary at: #endPosition);
		scriptResourceName: (aDictionary at: #scriptResourceName);
		sourceLine: (aDictionary at: #sourceLine);
		yourself! !

!CefJavaScriptException class categoriesForMethods!
fromDictionary:!public! !
!

!CefKeyEvent methodsFor!

asDictionary
	^Dictionary new
		at: #type put: self type;
		at: #modifiers put: self modifiers;
		at: #windows_key_code put: self windows_key_code;
		at: #native_key_code put: self native_key_code;
		at: #is_system_key put: self is_system_key;
		at: #character put: self character asString;
		at: #unmodified_character put: self unmodified_character asString;
		at: #focus_on_editable_field put: self focus_on_editable_field;
		yourself! !

!CefKeyEvent categoriesForMethods!
asDictionary!public! !
!

