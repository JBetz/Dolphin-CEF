| package |
package := Package name: 'Chromium Client'.
package paxVersion: 1;
	basicComment: ''.

package classNames
	add: #CefNavigationDestination;
	add: #CefNavigationEvent;
	add: #ContextMenuCommand;
	add: #ContextMenuConfiguration;
	add: #RemoteChromiumBrowser;
	add: #RemoteChromiumClient;
	add: #RemoteChromiumObject;
	yourself.

package methodNames
	add: #CefKeyEvent -> #asDictionary;
	add: #CefMouseEvent -> #asDictionary;
	add: #CefRect -> #asDictionary;
	add: #KernelLibrary -> #getProcessId:;
	add: 'CefJavaScriptException class' -> #fromDictionary:;
	add: 'CefRect class' -> #fromDictionary:;
	yourself.

package binaryGlobalNames: (Set new
	yourself).

package globalAliases: (Set new
	yourself).

package setPrerequisites: #(
	'..\Chromium Embedded Framework\Chromium Embedded Framework'
	'..\Core\Object Arts\Dolphin\Base\Dolphin'
	'..\Core\Contributions\Burning River\ExternalProcess\ExternalProcess'
	'..\..\Dolphin-SDL\SDL3\SDL3').

package!

